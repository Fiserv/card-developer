---
openapi: 3.0.0
info:
  title: Accounts
  description: Accounts Domain. All times will be in UTC standards.
  version: main-version
servers:
  - url: https://card.api.fiservapps.com/cs/accounts
    description: Production url
  - url: https://card-sandbox.api.fiservapps.com/cs/accounts
    description: Sandbox url
security:
  - BearerAuth: []
tags:
  - name: Details
    description: Search for Account Details.
  - name: Limits
    description: Search for Account limits for any specified account number.
  - name: Statements
    description: Search for Account statements.
  - name: Transaction
    description: Search for Account transactions.
paths:
  /v1/accounts/statements/search:
    post:
      tags:
        - Statements
      x-child-product-name: Accounts
      x-proxy-name: Statements
      x-group-name: Get Statements
      summary: Search for statement information given the account!!!!! Applies to credit only.
      operationId: getStatements
      parameters:
        - name: x-fapi-userId
          in: header
          description: User on behalf of whom the request is being made. userId must be alphanumeric.
          required: false
          style: simple
          explode: false
          schema:
            type: string
          example: JOHNDOE
        - $ref: '#/components/parameters/authorizationParam'
        - $ref: '#/components/parameters/clientIdParam'
        - name: filter
          in: query
          description: |
            Retrieve summary or detail information of statement. Default will be 'summary'.  
            
            filter value 'summary' only includes the following fields  
            - cardNumber  
            - statementDate  
            - statementEndingBalance  
            - delinquentAmount 
            - statementFinanceChargeAmount  
            - minimumPaymentDueAmount  
            - paymentDueDate  
            
            filter value of 'detail' includes all the fields in the response.
          required: false
          style: form
          explode: true
          schema:
            type: string
          example: summary
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/StatementSearchRequest'
        required: true
      responses:
        "200":
          description: OK
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/StatementSearchResponse'
              examples:
                Statement Search - Summary:
                  $ref: '#/components/examples/StatementSearchResponseSummary' 
                Statement Search - Detail:
                  $ref: '#/components/examples/StatementSearchResponseDetail'
        "206":
          description: Partial Content
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/StatementSearchResponse'
        "400":
          description: Invalid Request
          headers:
            x-fapi-interaction-id:
              $ref: '#/components/headers/interactionId'
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/4xxError'
              examples:
                StatementSearch400:
                  $ref: '#/components/examples/StatementSearch400'
        "401":
          description: Unauthorized access
          headers:
            x-fapi-interaction-id:
              $ref: '#/components/headers/interactionId'
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/4xxError'
              examples:
                StatementSearch401:
                  $ref: '#/components/examples/StatementSearch401'
        "403":
          description: Forbidden
          headers:
            x-fapi-interaction-id:
              $ref: '#/components/headers/interactionId'
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/4xxError'
              examples:
                StatementSearch403:
                  $ref: '#/components/examples/StatementSearch403'
        "404":
          description: Not Found
          headers:
            x-fapi-interaction-id:
              $ref: '#/components/headers/interactionId'
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/4xxError'
              examples:
                StatementSearch404:
                  $ref: '#/components/examples/StatementSearch404'
        "405":
          description: Method Not Allowed
          headers:
            x-fapi-interaction-id:
              $ref: '#/components/headers/interactionId'
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/4xxError'
              examples:
                StatementSearch405:
                  $ref: '#/components/examples/StatementSearch405'
        "406":
          description: Unacceptable
          headers:
            x-fapi-interaction-id:
              $ref: '#/components/headers/interactionId'
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/4xxError'
