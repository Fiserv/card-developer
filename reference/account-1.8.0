          description: Number of plastic cards issued which are associated to the account.
          example: 1
        prepayCode:
          type: string
          description: |
            Indicates if prepayment is allowed for the account.
            - 0 - Not Allowed.  
            - 9 - Allowed for all transactions.
          example: "0"
        secured:
          type: string
          description: |
            Indicates whether the account is secured.
          enum:
            - NOT_SECURED
            - SECURED
            - SECURED_BY_AGENT_FI
          example: NOT_SECURED
        serviceMember:
          type: boolean
          description: "Indicates if the account type is service member/military."
          example: false
