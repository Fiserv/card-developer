    AccountSearchResponse:
      title: AccountSearchResponse
      type: object
      properties:
        accountType:
          type: string
          description: |
            The type of the account.
            - Control 
            - Individual Pay  
            - Sub-Account
            - Consumer
          example: Consumer
        description:
          type: string
          description: The product description associated with the account.
          example: VISA Classic
        customerName:
          type: string
          description: Customer name associated with the account.
          example: Doe, Jesse H
        association:
          type: string
          description: The type of user linked with the account.
          enum:
            - PRIMARY
            - SECONDARY
            - AUTHORIZED_USER
            - GUARANTOR_OR_CO-SIGNER
          example: PRIMARY
        accountStatus:
          type: string
          description: Status of the account.
          enum:
            - NORMAL
            - AUTHORIZATION_PROHIBITED
            - BANKRUPT
            - CLOSED
            - FROZEN
            - INTEREST_PROHIBITED
            - REVOKED
            - CHARGED_OFF
          example: NORMAL
        accountStatusReason:
          type: string
          description: |
            Reason for the account status.
          
            - 00 - Normal  
            - 10 - Natural disaster  
            - 35 - Account in Collections  
            - 68 - Legally paid in full for less than the full balance  
            - 80 - Charged off account paid in full  
            - 85 - Closed due to Adaptive Control overlimit strategy  
            - 86 - Closed due to Adaptive Control delinquent strategy  
            - 87 - Closed due to Adaptive Control reissue strategy  
            - 88 - Fraud  
            - 89 - Bankrupt  
            - 91 - Closed due to account transfer  
            - 90 - Closed due to delinquency action code  
            - 92 - Closed in the reissue process due to delinquent and overlimit  
            - 93 - Closed in the reissue process due to REISSUE TERMS  
            - 94 - Closed in the reissue process due to INCLUDE/EXCLUDE STATES  
            - 95 - Closed in the reissue process due to an external status other than 'A' or space  
            - 96 - Closed in the reissue process due to inactive option  
            - 97 - Closed by customer  
            - 98 - Closed by FI  
            - 99 - Abandoned  
            - nn - Undefined  
          example: 00–Normal
        accountTransferStatus:
          type: string
          description: |
            Type of account transfer performed.
            - NO_TRANSFER_INVOLVEMENT - no transfer involvement on this account
            - ORIGINAL_TRANSFER_ACCOUNT - original transfer account (aka from account)
            - NEW_TRANSFER_ACCOUNT - new transfer account (aka to account)
            - ORIGINAL_ACCOUNT - original account (aka from combined account)
            - NEW_COMBINED_ACCOUNT - new combined account (aka to combined account)
          enum:
            - NO_TRANSFER_INVOLVEMENT
            - ORIGINAL_TRANSFER_ACCOUNT
            - NEW_TRANSFER_ACCOUNT
            - ORIGINAL_ACCOUNT
            - NEW_COMBINED_ACCOUNT
          example: NO_TRANSFER_INVOLVEMENT
        accountUsageStatus:
          type: string
          description: | 
            Represents current internal status of account.
            - NEVER_ACTIVE - Never active
            - PREVIOUSLY_ACTIVE - Account has been active but is not currently active
            - ACTIVE - Currently active account
            - DELINQUENT_ACCOUNT - Currently delinquent account or an account delinquent in its last three cycles
          enum:
            - NEVER_ACTIVE
            - PREVIOUSLY_ACTIVE
            - ACTIVE
            - DELINQUENT_ACCOUNT
          example: NEVER_ACTIVE
        authorizationCode:
          type: string
          description: Authorization approval code for the transaction.
          example: "0"
        automaticPaymentCode:
          type: string
          description: Payment code for the account.
          example: "4"
        automaticPaymentFlag:
          type: string
          description: |
            Indicates the use of the automatic payment option. If used, this determines if the payment is automatically deducted from a cardholder’s checking or savings account. 
            This flag also determines the production of hard copy debit memos or Automated Clearinghouse (ACH) tapes.  
            - blank - Do not use the automatic payment option  
            - 0 - Use the automatic payment option. Create debit memos as hard copy.  
            - 1 - Reserved for future use.  
            - 2 - Use the automatic payment option. The Federal Reserve receives an ACH tape with offsets at the principal level. The System automatically deducts payment from the cardholder’s checking or savings account.  
            - 3 - Use the automatic payment option. The Federal Reserve receives an ACH tape with offsets at the agent level. The System automatically deducts payment from the cardholder’s checking or savings account.  
            - 4 - Use the automatic payment option. The principal receives an ACH tape with offsets at the principal level. The System does not automatically deduct payment from the cardholder’s checking or savings account. Use code 4 if you want to verify that funds exist in the cardholder’s checking or savings account before deducting the payment.  
            - 5 - Use the automatic payment option. The system receives an ACH tape with offsets at the principal level. The System automatically deducts payment from the cardholder’s checking or savings account.  
            - 6 - Use the automatic payment option. The system receives an ACH tape with offsets at the agent level. The System automatically deducts payment from the cardholder’s checking or savings account.  
            - 7 - Use the automatic payment option. The principal receives an ACH tape with offsets at the principal level. The System automatically deducts payment from the cardholder’s checking or savings account.  
            - 8 - Use the automatic payment option. The principal receives an ACH tape with offsets at the agent level. The System automatically deducts payment from the cardholder’s checking or savings account.  
            - 9 - Use the automatic payment option. The agent receives an ACH tape with offsets at the agent level. The System automatically deducts payment from the cardholder’s checking or savings account.
          example: 1
        cashBalance:
          type: string
          description: Account cash balance.
          example: "30.00"
        currentBalanceAmount:
          type: string
          description: Current balance of the account.
          example: "3000.00"
        coveredByMilitaryLendingAct:
          type: boolean
          description: Indicates if the borrower is covered under the Military Lending Act.
          example: true
        expirationDate:
