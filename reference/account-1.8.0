    UpdateCashLimit:
      title: CashLimit
      type: object
      properties:
        limitType:
          type: string
          description: "Indicates type of cash limit."
          enum:
            - AMOUNT
            - PERCENT
        amount:
          type: integer
          format: int64
          minimum: 0
          maximum: 9999999999
          description: "Cash limit amount, provide in whole monetary units. If the limitType is AMOUNT, this field is required."
          example: '2000'
        percent:
          type: integer
          minimum: 0
          maximum: 100
          description: "Provide the percent of the credit limit used to calculate the cash limit. If limitType is PERCENT, this field is required."
          example: '25'
    UpdateTemporaryCreditLimit:
      title: TemporaryLimit
      type: object
      properties:
        setLimit:
          type: boolean
          description: "Provide 'true' in the setLimit to set a temporary credit limit; the amount and endDate are required.  If a temporary credit limit already exists (setLimit already set to true), the user can provide an update to the amount and endDate."
          example: true
        amount:
          type: integer
          format: int64
          minimum: 0
          maximum: 9999999999999999
          description: "Temporary amount, provide in whole monetary units. Required if the setLimit is set to 'true'."
          example: '3500'
        endDate:
          type: string
          maxLength: 10
          description: "Date temporary limit expires. Must be in UTC format, Must be equal to or greater than today's processing date; maximum date is 2049-12-30.  Required if temporary 'amount' is updated."
          format: "Date"
          example: '2022-06-30'
        limitApplied:
          type: boolean
          description: "Set the limitApplied to 'false' to remove an existng temporary credit limit.  Do not provide the amount and endDate, the system will  resets the permanent credit limit automatically."
          example: false
    AccountLimitSearchRequest:
      type: object
      required:
        - accountNumber
      properties:
        
        accountNumber:
          type: string
          description: Account number of the cardholder.
          
          example: 12345678
    
    AccountLimitSearchResponse:
      type: object
      properties:
        currentBalanceAmount:
          type: string
          description: "Current balance on the account."
          example: "3000.00"
        creditLimitAmount:
          type: string
          description: "Permanent credit limit for the account."
          example: "3000.00"
        availableCreditAmount:
          type: string
          description: "Credit available for the account."
          example: "3000.00"
        cashLimitAmount:
          type: string
          description: "Amount of the credit limit the account holder or cardholder can withdraw."
          example: "3000.00"
        
        availableCashAmount:
          type: string
          description: "Cash available for the account."
          example: "3000.00"
        creditLineMaximumAmount:
          type: string
          description: "Maximum credit limit allowed for accounts in whole monetary units."
          example: "3000.00"
        temporaryCreditLine:
          type: boolean
          description: "Indicates if credit limit is temporary or not.Possible values - true or false."
          example: "true"
        previousCreditLimitAmount:
          type: string
          description: "Previous temporary credit set up."
          example: "2000.00"
        temporaryCreditLimitAmount:
          type: string
          description: "Temporary line of credit for the account."
          example: "200.00"
        temporaryCreditLineEndDate:
          type: string
          description: "Date on which the temporary line of credit expires. This date must be equal to or greater than today's processing date."
          format: date
          example: "2023-09-09"
        billingCycle:
          type: string
          description: "Billing cycle for the account.Possible values - 1-28"
          example: "10"
        
        billingLevel:
          type: string
          description: "Indicates whether billing occurs for the account at the company level (consolidated) or at the individual account level.Possible values - Blank/space,Consolidated and Individual. Billing level is not allowed for Consumer/Primary/Control Account."
          example: "Individual"
        lastpermanentCreditLimit:
          type: string
          description: "Previous permanent credit limit assigned to the account."
          example: "1000.00"
        lastPermanentCreditLimitDate:
          type: string
          description: "Date on which the previous credit limit was assigned to the Account."
          format: date
          example: "2022-09-08"
        highBalanceDate:
          type: string
          description: "Date on which the account reached the highest balance."
          format: date
          example: "2022-09-07"
        highBalanceAmount:
          type: string
          description: Highest balance reached by the account.
          example: 2000.00
