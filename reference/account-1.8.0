  /v1/accounts/search:
    post:
      tags:
        - Details
      x-child-product-name: Accounts
      x-proxy-name: Account
      x-group-name: Get Account
      summary: Returns account information for the given account number based on the filter criteria passed which could be either 'summary' or 'detail'. Applies for credit only.
      operationId: getAccount
      parameters:
        - $ref: '#/components/parameters/authorizationParam'
        - $ref: '#/components/parameters/userIdParam'
        - $ref: '#/components/parameters/clientIdParam'
        - name: filter
          in: query
          description: |
            Retrieve summary or detail response for account search results. Defaults to 'summary'. 
            
            filter value of 'summary' only includes these fields  
            - accountType  
            - description 
            - customerName  
            - association  
            - accountStatus  
            
            filter value of 'detail' includes all the fields in the response.
          required: false
          style: form
          explode: true
          schema:
            type: string
          example: summary
      requestBody:
        description: accountSearchRequest
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/AccountSearchRequest'
        required: true
      responses:
        "200":
          description: OK
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/AccountSearchResponse'
              examples:
                Account Search - Summary:
                  $ref: '#/components/examples/AccountSearchResponseSummary'
                Account Search - Detail:
                  $ref: '#/components/examples/AccountSearchResponseDetail'           
        "400":
          description: Invalid Request
          headers:
            x-fapi-interaction-id:
              $ref: '#/components/headers/interactionId'
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/4xxError'
